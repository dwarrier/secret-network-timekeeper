{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InfoResponse",
  "type": "object",
  "required": [
    "curr_hash",
    "curr_offset",
    "min_difficulty",
    "min_update_length",
    "start_height"
  ],
  "properties": {
    "curr_hash": {
      "type": "string"
    },
    "curr_offset": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    },
    "min_difficulty": {
      "type": "string"
    },
    "min_update_length": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    },
    "start_height": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    }
  }
}
